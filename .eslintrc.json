{ 
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended"],
    "overrides": [
      {
        "files": ["./*.js"],
        "rules": {
          "no-unused-vars": "warn",
          "eqeqeq": ["warn", "smart"],
          "no-empty": "warn"
        }
      }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    }
}
